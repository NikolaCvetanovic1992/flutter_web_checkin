{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.freezed.dart": true,
    "**/*.g.dart": true
  },
  "command-runner.commands": {
    "build_runner delete-conflicting-outputs": "flutter pub run build_runner build --delete-conflicting-outputs",
    "build_runner": "flutter pub run build_runner build",
    "flutter pub get": "flutter pub get",
    "flutter clean": "flutter clean",
    "slang": "flutter pub run slang",
    "generate i18n json files": "dart run ./ourglass/packages/ourglass_slang/bin/ourglass_slang.dart"
  },
  "java.configuration.updateBuildConfiguration": "interactive"
}
